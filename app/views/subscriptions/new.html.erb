<%= form_for @subscription, remote: true, :html => {:'data-type' => 'json', :multipart => true} do |f| %>
    <%= f.label :email, 'Оповестить об открытии университета' %>
    <%= f.text_field :email, placeholder: 'email' %>
    <%= f.submit 'Оповестить', id: 'subscribe-button' %>
<% end %>